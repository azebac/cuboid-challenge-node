{
  "name": "fsl-challenge",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "12.18.2",
    "npm": "6.14.5"
  },
  "scripts": {
    "preinstall": "npx check-engine",
    "start": "node dist/index.js",
    "predevelop": "npm run db:migrate; npm run db:seed",
    "dev": "npx nodemon src/index.js",
    "lint": "eslint .",
    "prettier": "./node_modules/.bin/prettier --write '{src,tools,tests,knex}/**/*.js'",
    "pretest": "NODE_ENV=test npm run db:rollback:all && NODE_ENV=test npm run db:migrate",
    "pretest:CI": "NODE_ENV=test npm run db:migrate",
    "test": "jest --env=node",
    "test:CI": "NODE_OPTIONS='--max-old-space-size=3072' jest --maxWorkers=2",
    "watch": "jest --watchAll",
    "knex": "./node_modules/.bin/knex --esm",
    "db:migrate": "npm run knex -- migrate:latest",
    "db:migrate:make": "npm run knex -- migrate:make",
    "db:rollback": "npm run knex -- migrate:rollback",
    "db:rollback:all": "npm run knex -- migrate:rollback --all",
    "db:reset": "npm run db:rollback:all && npm run db:setup",
    "db:seed": "npm run knex -- seed:run",
    "db:setup": "npm run db:migrate && npm run db:seed"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.5",
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-winston": "^4.1.0",
    "faker": "^4.1.0",
    "http-status-codes": "^1.4.0",
    "knex": "^0.21.2",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "objection": "^2.2.1",
    "rosie": "^2.0.1",
    "sqlite3": "^5.0.0",
    "url-join": "^4.0.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-cup": "^2.0.3",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-sonarjs": "^0.5.0",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2"
  },
  "main": "src/index.js"
}
